def process_anfisa(query):\
    if query == '\uc0\u1089 \u1082 \u1086 \u1083 \u1100 \u1082 \u1086  \u1091  \u1084 \u1077 \u1085 \u1103  \u1076 \u1088 \u1091 \u1079 \u1077 \u1081 ?':\

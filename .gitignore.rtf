def process_friend(name, query):\
    if name in DATABASE:\
        city = DATABASE[name]\
        if query == '\uc0\u1090 \u1099  \u1075 \u1076 \u1077 ?':\
            return f'\{name\} \uc0\u1074  \u1075 \u1086 \u1088 \u1086 \u1076 \u1077  \{city\}'\
        elif query == '\uc0\u1082 \u1086 \u1090 \u1086 \u1088 \u1099 \u1081  \u1095 \u1072 \u1089 ?':   \
            if city in UTC_OFFSET:\
                return f'\uc0\u1058 \u1072 \u1084  \u1089 \u1077 \u1081 \u1095 \u1072 \u1089  \{what_time(city)\}'\
            else:\
                return f'<\uc0\u1085 \u1077  \u1084 \u1086 \u1075 \u1091  \u1086 \u1087 \u1088 \u1077 \u1076 \u1077 \u1083 \u1080 \u1090 \u1100  \u1074 \u1088 \u1077 \u1084 \u1103  \u1074  \u1075 \u1086 \u1088 \u1086 \u1076 \u1077  \{DATABASE[name]\}>'\
        else:\
            return '<\uc0\u1085 \u1077 \u1080 \u1079 \u1074 \u1077 \u1089 \u1090 \u1085 \u1099 \u1081  \u1079 \u1072 \u1087 \u1088 \u1086 \u1089 >'\
                    \
    else:\
        return f'\uc0\u1059  \u1090 \u1077 \u1073 \u1103  \u1085 \u1077 \u1090  \u1076 \u1088 \u1091 \u1075 \u1072  \u1087 \u1086  \u1080 \u1084 \u1077 \u1085 \u1080  \{name\}'\
\
\
def process_query(query):\
    elements = query.split(', ')\
    if elements[0] == '\uc0\u1040 \u1085 \u1092 \u1080 \u1089 \u1072 ':\
        return process_anfisa(elements[1])\
    else:\
        return process_friend(elements[0], elements[1])\
\

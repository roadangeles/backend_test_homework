        return f'\uc0\u1058 \u1074 \u1086 \u1080  \u1076 \u1088 \u1091 \u1079 \u1100 \u1103 : \{friends_string\}'\
    elif query == '\uc0\u1075 \u1076 \u1077  \u1074 \u1089 \u1077  \u1084 \u1086 \u1080  \u1076 \u1088 \u1091 \u1079 \u1100 \u1103 ?':\
        unique_cities = set(DATABASE.values())\
        cities_string = ', '.join(unique_cities)\
        return f'\uc0\u1058 \u1074 \u1086 \u1080  \u1076 \u1088 \u1091 \u1079 \u1100 \u1103  \u1074  \u1075 \u1086 \u1088 \u1086 \u1076 \u1072 \u1093 : \{cities_string\}'\
    else:\
        return '<\uc0\u1085 \u1077 \u1080 \u1079 \u1074 \u1077 \u1089 \u1090 \u1085 \u1099 \u1081  \u1079 \u1072 \u1087 \u1088 \u1086 \u1089 >'\
\
